{
  "version": "0.3.0",
  "license": "MIT",
  "nodeModulesDir": "auto",
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A --env-file --port=8000 _fresh/server.js",
    "start:cf": "npx wrangler dev --port 8000",
    "test": "deno test -A lib/",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@/": "./",
    "@cloudflare/vite-plugin": "npm:@cloudflare/vite-plugin@^1.19.0",
    "island-i18n": "jsr:@takahashim/island-i18n@^0.1.0",
    "fresh": "jsr:@fresh/core@^2.2.0",
    "preact": "npm:preact@^10.27.2",
    "@preact/signals": "npm:@preact/signals@^2.5.0",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.8",
    "arctic": "npm:arctic@^3.5.0",
    "@libsql/client": "npm:@libsql/client@^0.15.0",
    "vite": "npm:vite@^7.1.3",
    "nanoid": "npm:nanoid@^5.0.9",
    "@std/uuid": "jsr:@std/uuid@^1",
    "@std/assert": "jsr:@std/assert@^1",
    "tailwindcss": "npm:tailwindcss@^3.4.0",
    "daisyui": "npm:daisyui@^4.12.0",
    "autoprefixer": "npm:autoprefixer@^10.4.0",
    "wrangler": "npm:wrangler@^4.56.0"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  }
}
